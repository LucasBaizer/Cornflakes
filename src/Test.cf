package cornflakes.lang
public class Test

private var tuple: (i32, i32)

public constructor() {
	super()
	
	this.tuple = null
}

public constructor(tuple: (i32, i32)) {
	super()

	this.tuple = tuple
}

public static func test() {
	var x = Test()
	var y = (5, 5)
	
	var z = x + (5, 5)
	
	println!(z.tuple)
}

public static operator func +(test: Test, add: (i32, i32)) {
	return Test(add)
}