package game.server
public class Codes

public static const SUCCESS: i32 = 0
public static const INVALID_DATA_LENGTH: i32 = 1
public static const INVALID_OPCODE: i32 = 127

public static const REGISTER_USER: i32 = 0
public static const QUERY_POSITION: i32 = 1
public static const QUERY_USERNAME: i32 = 2
public static const QUERY_USER_LIST: i32 = 3

public static func isValidResponse(code: i32) -> bool {
	return code <= 4
}