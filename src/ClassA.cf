//Hello World written in Cornflakes

package cornflakes.lang
public class ClassA

use javax.swing.JFrame
use javax.swing.UIManager

public static function main() -> int {
	let lookAndFeel: string = "com.sun.java.swing.plaf.windows.WindowsLookAndFeel"
	UIManager.setLookAndFeel(lookAndFeel)
	set(JFrame("Hello, World!"))
	
	return 0
}

public static function set(JFrame obj) {
	obj.setVisible(true)
}
